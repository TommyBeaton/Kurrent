{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Lighthouse": {
    "Webhooks": [
      {
        "Name": "MainACR",
        "Path": "/acr/some-guid-would-go-here",
        "RequiredHeaders": [
          {
            "Name": "Authorization",
            "Value": "SomeTokenWouldGoHere"
          }
        ]
      }
    ],
    "Subscriptions":[
      {
        "Name": "Example",
        "WebhookName": "MainACR",
        "RepositoryName": "Example Repository",
        "Branch": "main"
      }
    ],
    "Repositories": [
      {
        "Name": "Example Repository",
        "Url": "www.github.com/example/example",
        "Username": "Example Username",
        "Password": "Example Password"
      }
    ]
  }
}
